  "peerDependencies": {
    "aws-sdk": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/debug": "0.0.31",
    "@types/faker": "^4.1.4",
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "@types/ramda": "^0.25.39",
    "faker": "^4.1.0",
    "inject-direnv": "^0.3.1",
    "jest": "^23.6.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
