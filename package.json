{
  "name": "dynalee",
  "version": "0.1.2",
  "description": "Typescript DynamoDB ORM",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/deptno/dynalee.git",
  "author": "Bonggyun Lee <deptno@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run compile; npm run rollup",
    "compile": "tsc -p .; tsc -p tsconfig.cjs.json",
    "rollup": "tsc -p tsconfig.config.json; rollup -c; node -e \"console.log(require('.'))\"",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=8.10"
  },
  "engineStrict": true,
  "dependencies": {
    "debug": "^4.1.0",
    "immer": "^1.7.3",
    "ramda": "^0.25.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0"
  },
  "peerDependencies": {
    "aws-sdk": "^2.342.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/debug": "^0.0.31",
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "@types/ramda": "^0.25.39",
    "jest": "^23.6.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "source-map-support": "^0.5.9",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
