{
  "name": "dynalee",
  "version": "0.1.0",
  "description": "Typescript DynamoDB ORM",
  "main": "dist/index.js",
  "repository": "https://github.com/deptno/dynalee.git",
  "author": "Bonggyun Lee <deptno@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json"
  },
  "engines": {
    "node": ">=8.10"
  },
  "engineStrict": true,
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "jest": "^23.6.0",
    "source-map-support": "^0.5.9",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.(test|spec).ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "dependencies": {
    "@types/debug": "^0.0.31",
    "@types/ramda": "^0.25.39",
    "debug": "^4.1.0",
    "immer": "^1.7.2",
    "ramda": "^0.25.0"
  },
  "peerDependencies": {
    "aws-sdk": "^2.342.0"
  }
}
